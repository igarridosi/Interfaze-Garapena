        <TextBlock HorizontalAlignment="Center" Margin="0,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="296" Text="Bienvenido al sitema" FontSize="30" FontWeight="Bold" />
        <TextBlock HorizontalAlignment="Left" Margin="111,93,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="51" Text="" x:Name="nombreWW"/>
        <TextBlock HorizontalAlignment="Left" Margin="111,133,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="67" Text="" x:Name="apellido1WW"/>
        <TextBlock HorizontalAlignment="Left" Margin="111,176,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="67" Text="" x:Name="apellido2WW"/>
        <TextBlock HorizontalAlignment="Left" Margin="111,217,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="51" Text="" x:Name="dniWW"/>

        <Button Content="Salir" HorizontalAlignment="Left" Height="27" Margin="400,275,0,0" VerticalAlignment="Top" Width="89" Click="Salir_Click"/>
        
    </Grid>
</Window>
